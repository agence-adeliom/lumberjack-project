<!doctype html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!--><html class="no-js no-svg" {{ site.language_attributes }}> <!--<![endif]-->
<head>
    <meta charset="{{ site.charset }}" />
    <meta name="description" content="{{ site.description }}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="format-detection" content="telephone=no">
    <link rel="author" href="{{ site.theme.link }}/humans.txt" />
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <link rel="profile" href="http://gmpg.org/xfn/11">
    <link href="{{ site.theme.link }}/node_modules/@fortawesome/fontawesome-pro/css/all.css" rel="stylesheet">
    {{function('wp_head')}}
    {% block stylesheet %}
    {% endblock %}

    {# <script>
        var GMAPS_API_KEY = "{{ config['gmap'] }}";
        var URL_AJAX = "{{ function('admin_url', 'admin-ajax.php') }}";
    </script> #}
</head>
<body class="{{ body_class }} font-sans leading-normal tracking-normal">
    <a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>

    {% block header %}
        {% if hide_header is not defined %}
            {% include 'components/navigations/header.html.twig' %}
        {% endif %}
    {% endblock %}

    <section id="content" role="main" class="py-4">
    {% block content %}
        <div class="container mx-auto">
            <p>Sorry, no content</p>
        </div>
    {% endblock %}
    </section>

    {% block footer %}
        {% if hide_footer is not defined %}
            {% include 'components/navigations/footer.html.twig' %}
        {% endif %}
    {% endblock %}
    {{ function('wp_footer') }}

    {% block javascript %}
    {% endblock %}
</body>
</html>
